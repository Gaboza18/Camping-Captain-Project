<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- resultType 등록 -->
	<typeAliases>
		<typeAlias type="com.camping.biz.dto.NoticeVO" alias="notice" />
		<typeAlias type="com.camping.biz.dto.QuestionsVO" alias="question" />
		<typeAlias type="com.camping.biz.dto.UsersVO" alias="users" />
	</typeAliases>

	<!-- SQL 매핑파일 등록 -->
	<mappers>
		<mapper resource="mappings/notice-mapping.xml" />
		<mapper resource="mappings/question-mapping.xml" />
		<mapper resource="mappings/users-mapping.xml" />
	</mappers>
	
</configuration>









